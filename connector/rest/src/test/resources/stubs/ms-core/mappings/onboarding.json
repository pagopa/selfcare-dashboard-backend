{
  "request": {
    "method": "POST",
    "urlPathPattern": "/onboarding/(?:subdelegates|operators)",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.institutionId"
      },
      {
        "matchesJsonPath": "$.productId"
      },
      {
        "matchesJsonPath": "$[?(@.users.size() == 1)]"
      }
    ]
  },
  "response": {
    "status": "201",
    "jsonBody": {},
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
