  "paths": {
    "/authorize/{institutionId}": {
      "get": {
        "tags": [
          "User Permission Controller"
        ],
        "summary": "Get permission for a user in an institution",
        "parameters": [
          {
            "name": "institutionId",
            "in": "path",
            "required": true,
            "schema": {
              "minLength": 1,
              "type": "string"
            }
          },
          {
            "name": "permission",
            "in": "query",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/PermissionTypeEnum"
            }
          },
          {
            "name": "productId",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "boolean"
                }
              }
            }
          },
          "401": {
            "description": "Not Authorized"
          },
          "403": {
            "description": "Not Allowed"
          }
        },
        "security": [
          {
            "SecurityScheme": []
          }
        ]
      }
    },
