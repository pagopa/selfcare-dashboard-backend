            "type" : "string",
            "pattern" : "^[a-zA-Z0-9 ]+$"
          },
          "required" : true
        }, {
          "name" : "productId",
          "in" : "query",
          "schema" : {
            "type" : "string"
          },
          "required" : true
        }, {
          "name" : "version",
          "in" : "query",
